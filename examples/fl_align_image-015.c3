import fl;

Fl_Align[*] align=
{
    FL_ALIGN_IMAGEOVERTEXT,
    FL_ALIGN_IMAGENEXTTOTEXT,
    FL_ALIGN_TEXTNEXTTOIMAGE,
    FL_ALIGN_IMAGEBACKDROP
};
const GAB=10;
fn void main()
{
    int n=align.len;

    fl::set_scheme("plastic");
    Fl_Window* win=fl::window_new(100,100,GAB*2+200,GAB*2+(n+1)*(50+GAB),"015-fl-align-image");
        for(int i=0;i<n;i++)
        {
            Fl_Button* btn=fl::button_new(GAB,GAB+i*(55+GAB),200,50," T e x t");
            btn.set_image(fl::pixmap_new(GRADIENT));// try SAMPLE
            btn.set_align(align[i]);

        }
    win.end();
    win.show();

    fl::run();
}

